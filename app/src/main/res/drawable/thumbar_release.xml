<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item >
        <shape android:shape="oval">
            <padding
                android:top="2dp"
                android:right="2dp"
                android:bottom="2dp"
                android:left="2dp" />
            <stroke
                android:dashWidth="2dp"
                android:dashGap="0dp"></stroke>
            <solid android:color="#00ABA9"/>
        </shape>
    </item>
</layer-list>